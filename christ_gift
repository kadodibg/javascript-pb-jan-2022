function giftsFromSanta(input) {
    let N = Number(input[0]);
    let M = Number(input[1]);
    let S = Number(input[2]);
    let result = "";

    for (let i = M; i >= N; i--) {
        // console.log(i)
        //    if(i === S){
        //     break;
        // }

        if (i % 2 === 0 && i % 3 === 0) {
            if (i === S) {
                break;
            }
            result += `${i} `
        }
    }
    console.log(result)
}
